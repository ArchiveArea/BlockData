{
    "name": "nhanaz/blockdata",
    "type": "library",
    "description": "A virion for PocketMine-MP that lets plugins store arbitrary block data",
    "license": "LGPL-3.0-or-later",
    "require": {
        "pocketmine/pocketmine-mp": "^5.0.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8.9"
    },
    "autoload": {
        "classmap": ["src"]
    },
    "extra": {
        "virion": {
            "spec": "3.0",
            "namespace-root": "NhanAZ\\BlockData"
        }
    }
}